{
  "kind": "collectionType",
  "collectionName": "products",
  "info": {
    "name": "product",
    "description": ""
  },
  "options": {
    "increments": true,
    "timestamps": true,
    "draftAndPublish": true
  },
  "attributes": {
    "title": {
      "type": "string",
      "required": true,
      "unique": true
    },
    "description": {
      "type": "text"
    },
    "price": {
      "type": "decimal"
    },
    "rating": {
      "type": "decimal",
      "required": true,
      "min": 0,
      "max": 5
    },
    "user": {
      "via": "products",
      "plugin": "users-permissions",
      "model": "user",
      "required": true,
      "autoPopulate": false
    }
  }
}
